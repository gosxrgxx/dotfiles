/* Confirm Dialog */

@import "colors.rasi"

* {
	font:				 	"Iosevka Nerd Font 10";
    text-color:             @light;
    background-color:       @dark;
}

window {
    width:      			225px;
    padding:    			25px;
    border:					0;
    border-radius:          0;
    location:               center;
}

entry {
    expand: 				true;
    text-color:				@gray;
}
